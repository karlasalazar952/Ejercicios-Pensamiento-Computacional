#Ejercicio 5
notas = ["Do", "Re", "Mi", "Fa", "Sol", "La", "Si"]
notas.remove("Fa")
notas.remove("Mi")
notas.insert(0,"Si")
notas.insert(1, "Si")
notas.insert(4, "Re")
notas.insert(5, "Do")
notas.insert(6, "Si")
notas.insert(7, "La")
notas.insert(9, "Sol")
notas.insert(12, "Si")
notas.insert(13, "La")
notas.insert(14, "La")
print(notas)

#Ejercicio 8
