#Ejercicio 4
a=int(input('Introduce un numero: '))
if a%2==0:
    a1='es un numero par'
elif a%2!=0:    
    a1='es un numero impar'
if a>0:
    ala=',positivo'
elif a<0 :
    ala=',negativo'
if a>100:
    a3='y mayor que 100'
elif a<100 :
    a3='y menor que 100'

print(a,a1,ala,a3)    

#Ejercicio 2
a=(input('Introduce un dia de la semana en minuscula: '))
if a=='lunes':
    print(a, 'es el dia 1 de la semana')
elif a=='martes':
    print(a, 'es el dia 2 de la semana')
elif a=='miercoles':
    print(a, 'es el dia 3 de la semana')
elif a=='jueves':
    print(a, 'es el dia 4 de la semana')
elif a=='viernes':
    print(a, 'es el dia 5 de la semana')
elif a=='sabado':
    print(a, 'es el dia 6 de la semana')
elif a=='domingo':
    print(a, 'es el dia 7 de la semana')
else:
    print('error')

#Ejercicio 7
a = input('Escriba un nombre: ')
arreglo = ['Jose Miguel', 'Carlos', 'Manuel', 'Memo' ]
if (a == arreglo):
    print('True')
else:
    print('False')
